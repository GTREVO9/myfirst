<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">

	<display-name>IBM_emp_0401MVC_Associations</display-name>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/TestDB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>

	<servlet>
		<servlet-name>EmpServlet</servlet-name>
		<servlet-class>com.emp.controller.EmpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmpServlet</servlet-name>
		<url-pattern>/emp/emp.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeptServlet</servlet-name>
		<servlet-class>com.dept.controller.DeptServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeptServlet</servlet-name>
		<url-pattern>/dept/dept.do</url-pattern>
	</servlet-mapping>
	
	

	
	
	<servlet>
		<servlet-name>Ccmt_ReportServlet</servlet-name>
		<servlet-class>com.ccmt_report.controller.Ccmt_ReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Ccmt_ReportServlet</servlet-name>
		<url-pattern>/ccmt_report/ccmt_report.do</url-pattern>
	</servlet-mapping>
	
	
	
	
		<servlet>
		<servlet-name>Course_CommentServlet</servlet-name>
		<servlet-class>com.course_comment.controller.Course_CommentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Course_CommentServlet</servlet-name>
		<url-pattern>/course_comment/course_comment.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Wish_ReportServlet</servlet-name>
		<servlet-class>com.wish_report.controller.Wish_ReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Wish_ReportServlet</servlet-name>
		<url-pattern>/wish_report/wish_report.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>WisServlet</servlet-name>
		<servlet-class>com.wis.controller.WisServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WisServlet</servlet-name>
		<url-pattern>/wis/wis.do</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>MemberDBReader</servlet-name>
		<servlet-class>com.member.controller.MemberDBReader</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MemberDBReader</servlet-name>
		<url-pattern>/MemberDBReader</url-pattern>
	</servlet-mapping>
	
	
	
	
	<servlet>
		<servlet-name>CourseServlet</servlet-name>
		<servlet-class>com.course.controller.CourseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CourseServlet</servlet-name>
		<url-pattern>/Course/course.do</url-pattern>
	</servlet-mapping>
	
	
	
	
	
	
	
	



	
	
	
	
	<!-- forTest -->
	<servlet>
		<servlet-name>test_controller</servlet-name>
		<servlet-class>forTest.Test_Controller_EmpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>test_controller</servlet-name>
		<url-pattern>/forTest/test_emp.do</url-pattern>
	</servlet-mapping>	








  

</web-app>
